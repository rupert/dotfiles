[include]
	path = ~/.gitconfig_local
[color]
	ui = auto
[push]
	default = simple
[filter "lfs"]
	clean = git-lfs clean %f
	smudge = git-lfs smudge %f
	required = true
[diff]
	renames = copies
[branch]
	autosetuprebase = always
[credential]
	helper = osxkeychain
[alias]
	st = status -sb
[core]
	excludesfile = ~/.gitignore_global
